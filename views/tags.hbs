	<article>
		<h1>#{{ type }}</h1>
      	<hr>
				<div class="listArticles">
					{{#each listArticles}}
					{{#if this.published}}
					<a href='/article/{{this._id}}'>
						<div class="boxArticle">
								<h2>{{this.title}}</h2>
								<em>{{this.author}}</em>
								<p>{{{this.resume}}}</p>
						</div>
					</a>
					{{/if}}
					{{/each}}
				</div>
    </article>
